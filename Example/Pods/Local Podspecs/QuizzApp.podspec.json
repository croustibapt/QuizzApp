  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*.{m,h}",
  "resource_bundles": {
    "QuizzAppXib": [
      "Pod/Assets/xib/**/*.{xib}"
    ],
    "QuizzAppImage": [
      "Pod/Assets/images/**/*.{png}"
    ],
    "QuizzAppENImage": [
      "Pod/Assets/en.lproj/**/*.{png}"
